<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Toyota" parallel="tests" thread-count="20">
	<listeners>
		<listener class-name="com.orasi.utils.Screenshot" />
	</listeners>

	<!-- MAC OS X 10.10 -->
	<test name="Mac_OS_X_10_10_Safari_8_toyota">
		<parameter name="browserUnderTest" value="safari" />
		<parameter name="environment" value="" />
		<parameter name="runLocation" value="remote" />
		<parameter name="browserVersion" value="8" />
		<parameter name="operatingSystem" value="MAC OS X 10.10" />
		<groups>
			<run>
				<include name="regression" />
			</run>
		</groups>
		<packages>
			<!-- <package name="toyota"></package> -->
			<package name="buyAToyota"></package>
		</packages>
	</test>
</suite> <!-- Toyota -->
